#include <iostream>
#include <cmath>
#include <random>
using namespace std;

bool Generator(uint64_t num)
{
    if (num<2)
        return false;
    if (num<4)
        return true;
    if ((num & 1)==0)
        return false;
    for(uint64_t i=3; i<=(uint64_t) sqrt(num); i+=2)
        if ((num % i) ==0)
            return false;
    return true;
}


int main(int argc, char **argv)
{
uint64_t num;
bool gen;
uint64_t a[3];
mt19937_64 rnd(clock());
for(int i=0; i<3; i++){
    gen=true;
    while(gen==true){
        num=rnd();
        if (Generator(num)==true){
            gen=false;
        }
        a[i]=num;
    }
    cout<<a[i]<<endl;
}
return 0;
}
